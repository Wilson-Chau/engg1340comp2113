#include <iostream>

using namespace std;

int main()
{
    for (int s1 = 1; s1 < 300; s1++){
        for (int s2 = s1; s2 < 300; s2++){
            for (int h = s2; h < 300; h++){
                if (s1 * s1 + s2 * s2 == h * h)
                    cout << s1 << " " << s2 << " " << h << endl;
            }
        }
    }

    return 0;
}
