#include <iostream>
#include <string>
using namespace std;

int main()
{
    string str, key;
    getline(cin, str);
    cin >> key;
    
    int pos = str.length();
    pos = str.rfind(key, pos - 1);
    
    if (pos == -1)
        cout << "Value not found" << endl;
    else
        cout << pos;
    
    while (pos != -1){
        pos = str.rfind(key, pos - 1);
        
        if (pos == -1)
            break;
        
        cout << " " << pos;
    }
    
    cout << endl;
    
    return 0;
}
