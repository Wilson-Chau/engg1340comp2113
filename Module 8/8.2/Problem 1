#include <iostream>

using namespace std;

int main()
{
    int N;
    cin >> N;
    
    double *numlist = new double [N];
    
    for (int i = 0; i < N; i++){
        cin >> numlist[i];
    }
    
    for (int i = 0; i < N; i++){
        cout << numlist[N - i - 1] << ' ';
    }
    
    cout << endl;

    return 0;
}
